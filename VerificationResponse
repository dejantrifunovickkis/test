package activate.privacy.model;


import android.text.TextUtils;

import androidx.annotation.Keep;

import com.google.gson.annotations.SerializedName;

@Keep
public class VerificationResponse {

    @SerializedName("timestamp")
    long secSinceFirstRun;
    @SerializedName("mac")
    int code;
    @SerializedName("expiration")
    String expiration;
    @SerializedName("hash")
    String hash;
    @SerializedName("key")
    String key;
    @SerializedName("success")
    boolean success;
    @SerializedName("willexpireindays")
    int willexpireindays;
    @SerializedName("trialDays")
    int trialDays;
    @SerializedName("isRegistered")
    boolean isregistered;

    String labelId;
    String mac;
    int deviceType;

    public boolean isApiRegistered() {
        return isregistered;
    }

    public VerificationResponse(long secSinceFirstRun, int code, String expiration, String hash, int willexpireindays, boolean isregistered, int trialDays) {
        this.secSinceFirstRun = secSinceFirstRun;
        this.code = code;
        this.expiration = expiration;
        this.success = success;
        this.willexpireindays = willexpireindays;
        this.trialDays = trialDays;
        this.isregistered = isregistered;
    }

    public VerificationResponse(String labelId, String key, String mac, int deviceType) {
        this.labelId = labelId;
        this.key = key;
        this.mac = mac;
        this.deviceType = deviceType;
    }

    public long getSecSinceFirstRun() {
        return secSinceFirstRun;
    }

    public void setSecSinceFirstRun(long secSinceFirstRun) {
        this.secSinceFirstRun = secSinceFirstRun;
    }

    public int getCode() {
        return code;
    }

    public void setCode(int code) {
        this.code = code;
    }

    public boolean isSuccess() {
        return success;
    }

    public int getTrialDays() {
        return trialDays;
    }

}
